{
  "name": "@zanix/asyncmq",
  "version": "0.3.11",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended",
        "jsr"
      ],
      "include": [
        "eqeqeq",
        "default-param-last",
        "camelcase",
        "no-await-in-loop",
        "no-const-assign",
        "no-eval",
        "no-non-null-asserted-optional-chain",
        "no-non-null-assertion",
        "no-self-compare",
        "no-sync-fn-in-async-fn",
        "no-throw-literal",
        "no-useless-rename"
      ]
    },
    "exclude": [
      ".dist"
    ],
    "plugins": [
      "jsr:@zanix/utils/linter/deno-zanix-plugin"
    ],
    "report": "pretty"
  },
  "fmt": {
    "exclude": [
      ".dist"
    ],
    "proseWrap": "always",
    "indentWidth": 2,
    "singleQuote": true,
    "lineWidth": 100,
    "useTabs": false,
    "semiColons": false
  },
  "imports": {
    "amqp": "npm:amqplib@^0.10",
    "@types/amqp": "npm:@types/amqplib@^0.10",
    "@std/path": "jsr:@std/path@0.224",
    "@std/assert": "jsr:@std/assert@0.224",
    "@std/testing": "jsr:@std/testing@1.0.16",
    "@zanix/validator": "jsr:@zanix/utils@2.2.x/validator",
    "@zanix/server": "jsr:@zanix/server@1.*",
    "@zanix/helpers": "jsr:@zanix/utils@2.2.x/helpers",
    "@zanix/logger": "jsr:@zanix/utils@2.2.x/logger",
    "@zanix/errors": "jsr:@zanix/utils@2.2.x/errors",
    "@zanix/workers": "jsr:@zanix/utils@2.2.x/workers",
    "@zanix/typings": "jsr:@zanix/utils@2.2.x/types",
    "@zanix/datamaster": "jsr:@zanix/datamaster@0.4.x/core",
    "shared/": "./src/shared/",
    "typings/": "./src/typings/",
    "utils/": "./src/utils/",
    "modules/": "./src/modules/"
  },
  "exports": {
    ".": "./mod.ts",
    "./worker": "./src/modules/worker/e-process.ts",
    "./core": "./src/modules/core.ts"
  },
  "nodeModulesDir": "auto",
  "publish": {
    "exclude": [
      ".github",
      "src/@tests"
    ]
  },
  "test": {
    "include": [
      "src/@tests/**/*.test.ts"
    ]
  }
}
